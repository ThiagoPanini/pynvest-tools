{
	"Version": "2012-10-17",
	"Statement": [
        {
			"Sid": "SQSPollMessages",
			"Effect": "Allow",
			"Action": [
        		"sqs:ReceiveMessage",
				"sqs:DeleteMessage",
				"sqs:GetQueueAttributes"
			],
			"Resource": [
                "arn:aws:sqs:${region_name}:${account_id}:pynvest-tickers-acoes-queue",
                "arn:aws:sqs:${region_name}:${account_id}:pynvest-tickers-fiis-queue"
            ]
		},
		{
			"Sid": "S3PutSoRData",
			"Effect": "Allow",
			"Action": [
				"s3:PutObject"
			],
			"Resource": [
				"arn:aws:s3:::${sor_bucket_name}/tbsor_fundamentus_*/*"
			]
		},
		{
			"Sid": "GlueAccessOnSoRTables",
			"Effect": "Allow",
			"Action": [
                "glue:GetDatabase",
				"glue:CreateTable",
				"glue:UpdateTable",
				"glue:GetTable",
                "glue:BatchCreatePartition"
            ],
			"Resource": [
                "arn:aws:glue:${region_name}:${account_id}:catalog",
                "arn:aws:glue:${region_name}:${account_id}:database/${sor_database_name}",
                "arn:aws:glue:${region_name}:${account_id}:table/${sor_database_name}/tbsor_fundamentus_*"
            ]
		}
	]
}